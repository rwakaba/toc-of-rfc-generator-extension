{
    "manifest_version": 2,
    "name": "TOC of RFC Generator",
    "description": "An Extension generate table of contents for any rfc pages",
    "version": "1.0.0",
    "page_action": {
        "default_icon": "icon-48.png",
        "default_title": "test",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/backgroundPage.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "notifications",
        "http://*/",
        "https://*/",
        "declarativeContent"
    ]
}